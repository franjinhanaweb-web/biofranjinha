# Configuração de exemplo para App Check
# Copie este arquivo para .env.local e configure suas chaves

# Site key do reCAPTCHA v3 (obtenha no Google reCAPTCHA Console)
REACT_APP_RECAPTCHA_SITE_KEY=sua_site_key_aqui

# Para desabilitar App Check temporariamente (se necessário)
# REACT_APP_DISABLE_APP_CHECK=true

# Para debug
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug
